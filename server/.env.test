# Node environment
NODE_ENV=test

# Express port for test server
PORT=5001

# Test database configuration
DATABASE_URL=postgres://testuser:testpass@test-db:5432/testdb
TEST_DB_NAME=testdb
TEST_DB_USER=testuser
TEST_DB_PASSWORD=testpass
TEST_DB_PORT=5433

# JWT config (using different secret for tests)
JWT_SECRET=test_secret_key
JWT_EXPIRES_IN=1h

# Bcrypt salt rounds (using lower number for faster tests)
BCRYPT_SALT_ROUNDS=4

# CORS origin endpoint
CORS_ORIGIN=http://localhost:3000